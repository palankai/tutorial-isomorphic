{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:bootstrap": "webpack --progress --config webpack.config.bootstrap.js",
    "build:dev": "webpack --progress",
    "build": "NODE_ENV=production webpack -p --progress",
    "test": "jest",
    "dev": "TMPDIR=/tmp babel-watch -x src/ --watch src/server/ src/server",
    "start": "NODE_ENV=production node /var/lib/frontend/server.js",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix"
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest"
  },
  "babel": {
    "presets": ["react", "env", "stage-3"],
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["./src"],
          "alias": {}
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "import"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "none",
          "ignoreRestSiblings": true
        }
      ],
      "class-methods-use-this": ["warn"],
      "array-callback-return": ["warn"]
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "jest": true
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack.config.client.js"
        }
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "airbnb-base"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "bootstrap-sass": "^3.3.7",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "ignore-styles": "^5.0.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "serialize-javascript": "^1.4.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.1.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-manifest-plugin": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-watch": "^2.0.7",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.6.1",
    "redux-devtools": "^3.4.0"
  }
}
